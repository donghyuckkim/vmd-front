{
  "name": "vmd-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "0.20.0",
    "bootstrap": "^4.5.2",
    "cross-env": "7.0.2",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "1.0.5",
    "package.json": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "3.14.4",
    "react-redux": "7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "prd": "export PORT=80 && env-cmd -f .env.production cross-env NODE_PATH=src react-scripts start",
    "dev": "export PORT=9091 && env-cmd -f .env.development cross-env NODE_PATH=src react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
